  "title": "Boom",
  "properties": {
    "Beleidsstatus": {
      "$id": "#/properties/Beleidsstatus",
      "title": "Beleidsstatus",
      "type": "string",
      "description": "Beleidsstatus is een functiecategorie bomen conform de richtlijnen NVTB, t.b.v. de bepaling van de monetaire waarde.",
      "enum": [
        "Structuurbepalend/hoofd(groen/bomen)structuur",
        "Beschermwaardig/monumentaal",
        "Geen specifieke status-verkorte omloop (tot ca. 20 jaar) en bomen 3e grootte",
        "Geen specifieke status-functionele laan- en parkbomen"
      ]
    },
    "BeoogdeOmlooptijd": {
      "$id": "#/properties/BeoogdeOmlooptijd",
      "title": "BeoogdeOmlooptijd",
      "type": "string",
      "description": "De potentieel haalbare omlooptijd, in relatie tot de standplaats (schatting).",
      "enum": [
        "30-50 jaar",
        "150-200 jaar",
        "20-30 jaar",
        "75-100 jaar",
        "50-75 jaar",
        "< 10 jaar",
        "Onbekend",
        "100-150 jaar",
        "10-20 jaar",
        ">200 jaar"
      ]
    },
    "Boombeeld": {
      "$id": "#/properties/Boombeeld",
      "title": "Boombeeld",
      "type": "string",
      "description": "Onderhoudssituatie van de boom.",
      "enum": [
        "Niet te beoordelen",
        "Verwaarloosd boombeeld",
        "Boombeeld regulier (HB)",
        "Achterstallig boombeeld",
        "Niet van toepassing",
        "Aanvaard boombeeld"
      ]
    },
    "Boombeschermer": {
      "$id": "#/properties/Boombeschermer",
      "title": "Boombeschermer",
      "type": "string",
      "description": "Wanneer een boombeschermer aanwezig is, wordt het GUID van het beheerobject Boombeschermer gekoppeld aan het object Boom.\nToelichting: Constructie, meestal van metaal, rondom het onderste  gedeelte van de stam, bedoeld ter bescherming van de stam."
    },
    "BoomBoomvoorziening": {
      "$id": "#/properties/BoomBoomvoorziening",
      "title": "BoomBoomvoorziening",
      "type": "string",
      "description": "Mogelijkheid om 1 of meerdere boomvoorzieningen bij een boom te registreren.",
      "enum": [
        "Boomverankering",
        "Beluchtingsdrain",
        "Kroonverankering",
        "Wortelwering",
        "Infiltratiesysteem",
        "Boompaal",
        "Gietrand",
        "Beluchtingssysteem permanent",
        "Wortelgeleiding",
        "Beluchtingssysteem aanlegfase",
        "Drainage"
      ]
    },
    "Boomgroep": {
      "$id": "#/properties/Boomgroep",
      "title": "Boomgroep",
      "type": "string",
      "description": "Aanduiding of de boom onderdeel is van een boomgroep.",
      "enum": [
        "Solitaire boom",
        "Boomweide",
        "Laanboom"
      ]
    },
    "BoomhoogteActueel": {
      "$id": "#/properties/BoomhoogteActueel",
      "title": "BoomhoogteActueel",
      "type": "integer",
      "description": "Hoogte van de boom in meters.\nEenheid: m"
    },
    "BoomhoogteklasseActueel": {
      "$id": "#/properties/BoomhoogteklasseActueel",
      "title": "BoomhoogteklasseActueel",
      "type": "string",
      "description": "Aanduiding van de boomhoogte in meters ingedeeld in vaste klassen.\nToelichting: Boomhoogte in meters, ingedeeld in vaste klassen,  gemeten vanaf het maaiveld tot de top van de boom\u00c2\u00b4, bij vormbomen gemeten tot de hoogste knot of de gewenste hoogte (Bron: RAW)",
      "enum": [
        "18 tot 24 m.",
        "6 tot 9 m.",
        "9 tot 12 m.",
        "tot 6 m.",
        "15 tot 18 m.",
        "Onbekend",
        "12 tot 15 m.",
        "24 m. en hoger"
      ]
    },
    "BoomhoogteklasseEindbeeld": {
      "$id": "#/properties/BoomhoogteklasseEindbeeld",
      "title": "BoomhoogteklasseEindbeeld",
      "type": "string",
      "description": "Aanduiding van de boomhoogte van het eindbeeld, in meters ingedeeld in vaste klassen.\nToelichting: Boomhoogte in meters, ingedeeld in vaste klassen,  gemeten vanaf het maaiveld tot de top van de boom\u00c2\u00b4, bij vormbomen gemeten tot de hoogste knot of de gewenste hoogte (Bron: RAW)"
    },
    "Boomspiegel": {
      "$id": "#/properties/Boomspiegel",
      "title": "Boomspiegel",
      "type": "string",
      "description": "Wanneer een boomspiegel aanwezig is, wordt het GUID van het beheerobject Boomspiegel gekoppeld aan het object Boom.\nToelichting: Boomspiegel: afgebakend oppervlak rondom de stam van een boom, dat niet is ingeplant."
    },
    "BoomTypeBeschermingsstatusPlus": {
      "$id": "#/properties/BoomTypeBeschermingsstatusPlus",
      "title": "BoomTypeBeschermingsstatusPlus",
      "type": "string",
      "description": "Nadere aanduiding voor de speciale status van de boom.",
      "enum": [
        "Bomenstructuur boom",
        "Vaste rust- en of verblijfplaats",
        "Cultuurhistorisch waardevol",
        "Jaarrond beschermd nest",
        "Onderdeel van monumentale boomstructuur",
        "Bijzondere snoeivorm",
        "Bomenlijst boom",
        "Dendrologisch waardevol",
        "Ecologisch waardevol",
        "Herdenkingsboom",
        "Bijzondere groeivorm",
        "Toekomstboom",
        "Verblijfplaats Vleermuizen",
        "Veteranenboom",
        "Natuurwetenschappelijk waardevol",
        "Beeldbepalende boom",
        "Vliegroute vleermuizen",
        "Zeldzaamheidswaarde"
      ]
    },
    "Boomveiligheidsklasse": {
      "$id": "#/properties/Boomveiligheidsklasse",
      "title": "Boomveiligheidsklasse",
      "type": "string",
      "description": "Aanduiding van de veiligheid van de boom, ingedeeld in vaste klassen.\nToelichting: Voor bosplantsoen met boomvormers is de mate van veiligheid van het beheerobject voor de omgeving relevant. Hiervoor is nog geen landelijk vastgestelde classificatie. Boomveiligheid: Aanduiding voor de veiligheid van personen, dieren, objecten en goederen in de nabijheid van een boom.",
      "enum": [
        "Attentieboom",
        "Niet te beoordelen",
        "Boom zonder gebreken",
        "Onbekend",
        "Risicoboom"
      ]
    },
    "Controlefrequentie": {
      "$id": "#/properties/Controlefrequentie",
      "title": "Controlefrequentie",
      "type": "string",
      "description": "Aanduiding van de frequentie van de controle van het beheerobject.\nToelichting: \"De frequentie van de boomveiligheidscontrole. Dit is de periodieke visuele controle van een boom in het kader van \u00e2\u0080\u0098de zorgplicht\u00e2\u0080\u0099 (voortkomend uit artikel 6:162 van het BW) ten behoeve van het vaststellen van een (potentieel toekomstig) risico dat de boom vormt voor zijn omgeving. \nDe term \u00e2\u0080\u0098boomveiligheidscontrole\u00e2\u0080\u0099 heeft betrekking op het gehele proces om \u00e2\u0080\u0098in het veld\u00e2\u0080\u0099 de benodigde gegevens te verkrijgen voor het logboek, zoals beschreven in de CROW Richtlijn Boomveiligheidsregistratie.\n\"",
      "enum": [
        "1 x per maand",
        "1 x per 6 jaar",
        "1 x per 5 jaar",
        "1 x per 2 jaar",
        "1 x per 8 jaar",
        "1 x per 4 jaar",
        "1 x per 10 jaar",
        "Onbekend",
        "3 x per jaar",
        "1 x per 3 jaar",
        "1 x per 9 jaar",
        "1 x per 7 jaar",
        "1 x per jaar",
        "1 x per halfjaar",
        "Niet van toepassing",
        "1 x per kwartaal"
      ]
    },
    "Feestverlichting": {
      "$id": "#/properties/Feestverlichting",
      "title": "Feestverlichting",
      "type": "string",
      "description": "Wanneer Feestverlichting aanwezig is, wordt het GUID van het beheerobject Feestverlichting gekoppeld aan het gekoppelde beheerobject."
    },
    "Groeifase": {
      "$id": "#/properties/Groeifase",
      "title": "Groeifase",
      "type": "string",
      "description": "Aanduiding van de groeifase van een boom.\nToelichting: Er is geen volledige eenduidigheid over de indeling, maar over het algemeen worden bij een boom zo\u00c2\u00b4n 4 groeifasen onderscheiden. Het onderscheid is gebaseerd op de verschillen in beheermaatregelen.",
      "enum": [
        "Niet te beoordelen",
        "Jeugdfase",
        "Onbekend",
        "Aanlegfase",
        "Volwassen fase",
        "Eindfase"
      ]
    },
    "Groeiplaatsinrichting": {
      "$id": "#/properties/Groeiplaatsinrichting",
      "title": "Groeiplaatsinrichting",
      "type": "string",
      "description": "Wanneer een groeiplaatsinrichting aanwezig is, wordt het GUID van het beheerobject Groeiplaatsinrichting gekoppeld aan het object Boom"
